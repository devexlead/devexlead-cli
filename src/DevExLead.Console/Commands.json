                {
                    "Name": "npm",
                    "Description": "npm Package Dependencies",
                    "Handler": "DevExLead.Modules.Export.Handlers.NpmHandler",
                    "Parameters": [
                        {
                            "Name": "path",
                            "Description": "Path to be scanned"
                        },
                        {
                            "Name": "format",
                            "Description": "'csv', 'xls' or 'json'",
                            "IsRequired": true,
                            "DefaultValue": "xls"
                        }
                    ]
                },
                {
                    "Name": "catalog",
                    "Description": "Software Catalog based on Kubernetes and Datadog Files",
                    "Handler": "DevExLead.Modules.Export.Handlers.CatalogHandler",
                    "Parameters": [
                        {
                            "Name": "path",
                            "Description": "Path to be scanned"
                        },
                        {
                            "Name": "format",
                            "Description": "'csv', 'xls' or 'json'",
                            "IsRequired": true,
                            "DefaultValue": "xls"
                        }
                    ]
                },
                {
                    "Name": "jira",
                    "Description": "JIRA Issues",
                    "Handler": "DevExLead.Modules.Export.Handlers.JiraHandler",
                    "Parameters": [
                        {
                            "Name": "path",
                            "Description": "Path where export file will be saved"
                        },
                        {
                            "Name": "format",
                            "Description": "'csv', 'xls' or 'json'",
                            "IsRequired": true,
                            "DefaultValue": "xls"
                        },
                        {
                            "Name": "jql",
                            "Description": "JQL",
                            "IsRequired": true,
                            "DefaultValue": ""
                        }
                    ]
                }
            ]
        },
